<div id="my-trips">

</div>

<% @trips.each do |trip| %>
  <h6 id="activity-title"class="small-3 small-centered columns"><%= trip.trip_name %></h6>
  <h6 id="activity-list" class="row small-6 small-centered columns"><% trip.activities.each do |activity| %>
    <a href="/activities/<%= activity['id'] %>"><%= activity['name'] %>     -----</a>
    <% end %></h6>
<% end %>
